# 1. source env-setup
# 2. export DCOS_PRODUCTION=false

if [ -n "$BASH_SOURCE" ] ; then
    BIN_DIR=$(dirname "$BASH_SOURCE")
elif [ $(basename -- "$0") = "env-setup" ]; then
    BIN_DIR=$(dirname "$0")
else
    BIN_DIR=$PWD/bin
fi

BASE_DIR="$BIN_DIR/.."

source ${BASE_DIR}/env/bin/env-setup
export DCOS_PRODUCTION=false
